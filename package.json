{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "generate:types": "run-p generate:types:*",
    "generate:types:cms": "graphql-codegen -r dotenv/config --config ./app/models/directus/codegen.config.ts",
    "build": "run-s generate:types remix:build",
    "dev": "run-s generate:types remix:dev",
    "typecheck": "tsc -b",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "vitest",
    "test:unit:run": "vitest run --coverage",
    "test:unit:changed": "vitest run --coverage --changed",
    "remix:build": "remix build",
    "remix:dev": "remix dev",
    "autotest": "pnpm generate:types && pnpm lint && pnpm test && pnpm typecheck",
    "start": "pm2 start --name=remix --update-env pnpm -- remix ",
    "stop": "pm2 stop remix && pm2 delete remix",
    "remix": "pnpm build && PORT=3006 remix-serve build"
  },
  "dependencies": {
    "@babel/core": "^7.21.4",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@mantine/carousel": "^6.0.5",
    "@mantine/core": "^6.0.5",
    "@mantine/hooks": "^6.0.5",
    "@mantine/modals": "^6.0.5",
    "@mantine/notifications": "^6.0.5",
    "@mantine/remix": "^6.0.5",
    "@remix-run/node": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "@tabler/icons-react": "^2.15.0",
    "@types/jest-axe": "^3.5.5",
    "bson-ext": "^4.0.3",
    "clsx": "^1.2.1",
    "embla-carousel-autoplay": "^7.1.0",
    "embla-carousel-react": "^7.1.0",
    "graphql": "^16.6.0",
    "graphql-request": "5.1.0",
    "graphql-tag": "^2.12.6",
    "isbot": "^3.6.5",
    "node-cache": "^5.1.2",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-crossfade-simple": "^1.0.7",
    "react-dom": "^18.2.0",
    "react-tiny-crossfade": "^2.2.0",
    "schema-ast": "^2.0.0",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/client-preset": "^3.0.0",
    "@graphql-codegen/introspection": "^3.0.1",
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.8",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest-axe": "^7.0.1",
    "jsdom": "^21.1.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "typescript-graphql-request": "^4.4.6",
    "vite": "^4.2.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.30.1"
  },
  "engines": {
    "node": ">=14"
  }
}
